43a\u043e\u0442\u043e\u0440\u043e\u0435 \u0432 \u0431\u0443\u0434\u0443\u0449\u0435\u043c  \u043f\u0440\u043e\u044f\u0432\u0438\u0442\u0441\u044f \u043a\u0430\u043a \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0437\u0430\u0431\u043e\u043b\u0435\u0432\u0430\u043d\u0438\u0435. \n\u0421\u0443\u0442\u044c \u0433\u0430\u0440\u043c\u043e\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u0447\u0430\u043a\u0440 \u2014 \u0430\u043a\u0442\u0438\u0432\u0438\u0437\u0430\u0446\u0438\u044f \u0442\u043e\u043d\u043a\u0438\u0445 \u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043c\u0435\u0445\u0430\u043d\u0438\u0437\u043c\u043e\u0432,\n\u0441\u0430\u043c\u043e\u0440\u0435\u0433\u0443\u043b\u044f\u0446\u0438\u044f, \u0441\u0430\u043c\u043e\u0432\u043e\u0441\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u0435 \u043f\u0441\u0438\u0445\u0438\u043a\u0438 \u0438 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043c\u0430 - \u044d\u0442\u043e \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u0439\n\u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u043c\u0430\u0441\u0441\u0430\u0436 \u0447\u0430\u043a\u0440. \n\u0413\u0430\u0440\u043c\u043e\u043d\u0438\u0437\u0430\u0446\u0438\u044f \u0447\u0430\u043a\u0440  \u0432\u044b\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0435\u0442 \u0432\u0441\u0435 \u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0441\u043b\u043e\u0438, \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u0442\u0441\u044f \u0432 \u0431\u0430\u043b\u0430\u043d\u0441\n\u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0438 \u043f\u0441\u0438\u0445\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0442\u0435\u043b\u043e, \u0441\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u0431\u043b\u043e\u043a\u0438 \u0438 \u043d\u0430\u043f\u0440\u044f\u0436\u0435\u043d\u0438\u044f \u0432\u043e \u0432\u0441\u0435\u043c \u0442\u0435\u043b\u0435 \u0438\n\u0441\u043e\u0437\u043d\u0430\u043d\u0438\u0438.\n\n\u0423\u043b\u0443\u0447\u0448\u0430\u0435\u0442\u0441\u044f \u043e\u0431\u0449\u0435\u0435 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u043a\u0438,  \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043c \u0433\u043b\u0443\u0431\u043e\u043a\u043e  \u0440\u0430\u0441\u0441\u043b\u0430\u0431\u043b\u044f\u0435\u0442\u0441\u044f. \n\u041a\u0430\u043a \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0441\u0435\u0430\u043d\u0441? \n\u041a\u043b\u0438\u0435\u043d\u0442 \u0434\u0435\u043b\u0430\u0435\u0442 \u0437\u0430\u043f\u0440\u043e\u0441 \u043d\u0430 \u0442\u043e, \u0447\u0442\u043e \u0445\u043e\u0447\u0435\u0442 \u043f\u0440\u043e\u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c, \u0431\u0443\u0434\u044c \u044d\u0442\u043e \u0431\u043e\u043b\u0438 \u0432 \u0442\u0435\u043b\u0435 \u0438\u043b\u0438\n\u043d\u0435\u0440\u0430\u0437\u0440\u0435\u0448\u0451\u043d\u043d\u044b\u0435 \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u043d\u044b\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b \u0438\u043b\u0438 \u0432\u043e\u0441\u043f\u043e\u043c\u0438\u043d\u0430\u044f, \u043a\u0430\u043a \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0435 \u0437\u0434\u0435\u0441\u044c \u0438 \u0441\u0435\u0439\u0447\u0430\u0441,\u0442\u0430\u043a \u0438 \u0441\u043b\u0443\u0447\u0438\u0432\u0448\u0438\u0435\u0441\u044f \u0432 \u043f\u0440\u043e\u0448\u043b\u043e\u043c.\n\n\u0412\u044b \u043b\u0435\u0436\u0438\u0442\u0435 \u043d\u0430 \u043a\u0443\u0448\u0435\u0442\u043a\u0435, \u043c\u044f\u0433\u043a\u043e\u0435 \u0432\u043e\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435 \u043d\u0430 \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0442\u0435\u043b\u043e \u0432 \u0437\u043e\u043d\u0430\u0445 \u0447\u0430\u043a\u0440 \u0430 \u0442\u0430\u043a\u0436\u0435  \u0440\u0443\u043a\u0438, \u043d\u043e\u0433\u0438, \u0433\u043e\u043b\u043e\u0432\u0430.\n\u0421 \u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u043a\u043e\u0439 (\u0442\u043e\u043d\u043a\u043e\u0439 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0449\u0435\u0439 \u0443\u043c\u0430, \u043f\u0441\u0438\u0445\u0438\u043a\u043e\u0439, \u044d\u043c\u043e\u0446\u0438\u044f\u043c\u0438, \u0433\u043b\u0443\u0431\u0438\u043d\u043d\u043e\u0439\n\u043f\u043e\u0434\u0441\u043e\u0437\u043d\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u043f\u0430\u043c\u044f\u0442\u044c\u044e, \u043c\u044b\u0448\u043b\u0435\u043d\u0438\u0435\u043c \u0438 \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u043d\u043e\u0441\u0442\u044c\u044e.\n\u0421\u0431\u0440\u043e\u0441 \u0441\u043e\u0431\u0440\u0430\u043d\u043d\u043e\u0439 \u043d\u0435\u0433\u0430\u0442\u0438\u0432\u043d\u043e\u0439 \u044d\u043d\u0435\u0440\u0433\u0438\u0438 \u0438  \u0432\u044b\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u043d\u0438\u0435 \u043e\u0431\u0449\u0435\u0439 \u044d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u043a\u0438 \u0447\u0430\u043a\u0440\n \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u043c \u043e\u0440\u0433\u0430\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u043c  \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u043e\u043c. \n\u0410\u043a\u0442\u0443\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f  \u0437\u0430\u043f\u0440\u043e\u0441\u0430 \u043d\u0430 \u0441\u0431\u0440\u043e\u0441 \u043f\u043e\u0434\u0441\u043e\u0437\u043d\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043d\u0430\u043f\u0440\u044f\u0436\u0435\u043d\u0438\u044f \u0438 \u0432\u043e\u0441\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0438\u0435\n\u0440\u0435\u0441\u0443\u0440\u0441\u043d\u043e\u0433\u043e \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044f.\n\n\u0421\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u0441\u0435\u0430\u043d\u0441\u0430 20 000 \u0442\u043d.\n\n\u0415\u0441\u043b\u0438 \u043f\u043e\u0447\u0443\u0432\u0441\u0442\u0432\u0443\u0435\u0442\u0435, \u0447\u0442\u043e \u0